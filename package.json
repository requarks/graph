{
  "name": "graph",
  "version": "1.0.0",
  "description": "GraphQL API for Requarks projects",
  "scripts": {
    "start": "node server",
    "dev": "nodemon -V --delay 1",
    "test": "eslint --ext .js ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Requarks/graph.git"
  },
  "keywords": [
    "api",
    "graphql"
  ],
  "author": "Nicolas Giard",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Requarks/graph/issues"
  },
  "homepage": "https://github.com/Requarks/graph#readme",
  "dependencies": {
    "apollo-server-express": "2.4.8",
    "auto-load": "3.0.4",
    "bluebird": "3.5.4",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "country-language": "0.1.7",
    "cron": "1.7.0",
    "custom-error-instance": "2.1.1",
    "express": "4.16.4",
    "fs-extra": "7.0.1",
    "graphql": "14.2.1",
    "graphql-rate-limit-directive": "1.0.1",
    "ioredis": "4.9.0",
    "js-yaml": "3.13.1",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "pug": "2.0.3",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "semver": "6.0.0",
    "serve-favicon": "2.5.0",
    "uuid": "3.3.2",
    "validator": "10.11.0",
    "yargs": "13.2.2"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "graphql-schema-linter": "0.2.0",
    "graphql-tag": "2.10.1",
    "nodemon": "1.18.10"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "nodemonConfig": {
    "exec": "node ./server --dev | bunyan -o short",
    "ext": "js json graphql",
    "watch": [
      "server/"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  }
}
